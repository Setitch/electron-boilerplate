{
  "name": "electron-base-template",
  "productName": "electron-base-template",
  "version": "0.0.1",
  "description": "Template for Electron applications",
  "main": ".rsbuild/main/main.cjs",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "type": "module",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "boilerplate",
    "electron-forge",
    "rsbuild",
    "sqlite",
    "mantine",
    "mantine-colors",
    "react-query"
  ],
  "author": {
    "name": "Artur (Seti) Łabudziński"
  },
  "contributes": [
    {
      "https://github.com/flaviodelgrosso/reactronite.git": "Idea for project and initial code"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/setitch/electron-boilerplate.git"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "sqlite3"
    ],
    "patchedDependencies": {
      "electron-winstaller@5.4.0": "patches/electron-winstaller@5.4.0.patch",
      "@electron/packager@18.3.6": "patches/@electron__packager@18.3.6.patch"
    }
  },
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development electron-forge start",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "build": "electron-forge build",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --config eslint.config.ts",
    "lint:fix": "eslint --config eslint.config.ts --fix",
    "clean": "rimraf node_modules .rsbuild pnpm-lock.yaml",
    "postinstall": "husky",
    "postinstall:rebuild": "@electron/rebuild",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "release": "standard-version",
    "major": "npm run release -- --release-as major",
    "minor": "npm run release -- --release-as minor",
    "patch": "npm run release -- --release-as patch",
    "push-release": "git push --follow-tags origin master"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@mantine/core": "^8.1.1",
    "@mantine/form": "^8.1.2",
    "@mantine/hooks": "^8.1.1",
    "@mantine/notifications": "^8.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@tabler/icons-react": "^3.34.0",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "better-sqlite3": "^12.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "10.1.0",
    "electron-window-state": "^5.0.3",
    "filepond": "^4.30.4",
    "filepond-plugin-file-validate-size": "^2.2.8",
    "filepond-plugin-file-validate-type": "^1.2.8",
    "filepond-plugin-image-preview": "^4.6.11",
    "lucide-react": "^0.517.0",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-filepond": "7.1.3",
    "react-qr-code": "^2.0.16",
    "react-router-dom": "^7.6.2",
    "react-select": "^5.10.1",
    "sequelize": "6.37.7",
    "sequelize-typescript": "2.1.6",
    "sqlite3": "^5.1.7",
    "ts-node": "10.9.2",
    "typescript": "~5.8.3",
    "umzug": "^3.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron-forge/shared-types": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^3.7.2",
    "@eslint/js": "^9.29.0",
    "@rsbuild/core": "^1.3.22",
    "@rsbuild/plugin-react": "^1.3.2",
    "@rsbuild/plugin-svgr": "^1.2.0",
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "4.5.2",
    "classnames": "^2.5.1",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "electron": "36.5.0",
    "electron-devtools-installer": "^4.0.0",
    "electron-forge-plugin-rsbuild": "^1.0.2",
    "eslint": "^9.29.0",
    "eslint-plugin-oxlint": "^1.1.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unicorn": "^59.0.1",
    "husky": "9.1.7",
    "lint-staged": "^16.1.2",
    "neostandard": "^0.12.1",
    "pinst": "^3.0.0",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "ts-node": "10.9.2",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
